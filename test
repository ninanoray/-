		<table border="1">
			<caption>캡션입니다</caption>
			<thead>
				<tr><th>이름</th><th>HTML</th><th>CSS</th></tr>
			</thead>
			<tfoot>
				<tr><th>th는 굵게</th><th>80</th><th>70</th></tr>
			</tfoot>
			<tbody>
				<tr><td>td는 얇게</td><td>80</td><td>70</td></tr>
				<tr><td rowspan =3>아래병합</td><td colspan =2>오른쪽병합</td></tr>
				<tr><td rowspan =2>아래병합</td><td>D</td></tr>
				<tr><td>E</td></tr>
			</tbody>
		</table>

